<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.5/distro-source/core/src/gwt-module.dtd">
<module rename-to="GWTCommunicator">
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name="com.google.gwt.core.Core" />
    <inherits name='com.google.gwt.user.User' />
    <inherits name='com.google.gwt.dom.DOM' />
    <!--<inherits name="com.google.gwt.logging.Logging"/>-->

    <inherits name="com.google.gwt.query.QueryMin" />
    <inherits name="com.google.gwt.json.JSON"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class="com.outr.net.communicator.client.GWTCommunicator"/>

    <!-- Comment out the following before production builds -->

    <!-- User Agent -->
    <set-property name="user.agent" value="safari"/>
    <!--<set-property name="user.agent" value="gecko1_8"/>-->
    <!--<set-property name="user.agent" value="opera"/>-->
    <!--<set-property name="user.agent" value="ie9"/>-->

    <collapse-all-properties />
</module>